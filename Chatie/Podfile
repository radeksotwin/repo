# Facebook SDK
pod 'FBSDKLoginKit'

# Google Sign-In
pod 'GoogleSignIn', '< 5.0.0'
pod 'AppAuth'
pod 'GTMAppAuth'


pod 'MessageKit'
pod 'JGProgressHUD'
pod 'SDWebImage'

end
